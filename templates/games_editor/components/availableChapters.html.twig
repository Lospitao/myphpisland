<div class="availableChaptersList">
    <ul id="availableChaptersList" class="connectedSortable availableChapters">
        {% for availableChapter  in availableChapters  %}
            <li class="ui-state-highlight availableChapterElement" data-title="{{ availableChapter['title']  }}" data-uuid="{{ availableChapter['uuid'] }}">
                {{ availableChapter['title'] }}
                <a href="#"><i class="tiny material-icons availableChapter"  data-title="{{ availableChapter['title'] }}" data-uuid="{{ availableChapter['uuid'] }}" >add_circle</i></a>
            </li>

        {% endfor %}
    </ul>
</div>
