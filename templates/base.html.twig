
<!DOCTYPE html>

<head>

    <meta charset="UTF-8">

    <title>{% block title %}Welcome!{% endblock %}</title>
    {% block stylesheets %}


        <!--Jquery,-->

        <script
                src="https://code.jquery.com/jquery-3.5.1.min.js"
                integrity="sha256-9/aliU8dGd2tb6OSsuzixeV4y/faTqgFtohetphbbj0="
                crossorigin="anonymous">

        </script>

        <!-- Compiled and minified CSS -->
        <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0/css/materialize.min.css">
        <!-- Compiled and minified JavaScript -->
        <script src="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0/js/materialize.min.js"></script>
        <!-- Materialize icons -->
        <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
        <!--CSS stylesheet-->
        <link rel="stylesheet" type="text/css" href="{{ asset('css/style.css') }}">




    {% endblock %}
</head>
<body>

    <nav class="menu logged-user">
        <div class="nav-wrapper">
            {% if is_granted('IS_AUTHENTICATED_FULLY') %}
                <a href="#!" class="brand-logo"><img src="{{ '/uploads/' ~ app.user.profilePic }}" alt="" style="min-width:30px;
	                    width: 3.8%;"></a>
            {% endif %}
            <ul class="right hide-on-med-and-down">
                <li><a class="waves-effect waves-light btn"><i class="material-icons">person</i></a></li>
                <li><a class="waves-effect waves-light btn"><i class="material-icons">notifications</i></a></li>
                <li><a class="waves-effect waves-light btn"><i class="material-icons">settings</i></a></li>
                {% if is_granted('IS_AUTHENTICATED_FULLY') %}
                    <li>
                        <a class="waves-effect waves-light btn" href="{{ path('app_logout') }}">Logout</a>
                    </li>
                {% endif %}
            </ul>
        </div>
    </nav>

{% block body %}

{% endblock %}


{% block javascripts %}{% endblock %}
</body>

